import java.util.ArrayList;

public class corejava2 {

	public static void main(String[] args) {
		//arrayList
		
		ArrayList a = new ArrayList();
		a.add("Prashanna");
		a.add("Dhivyan");
		a.add("Letchu");
		System.out.println(a);
		System.out.println(a.get(0));
		
		for(Object s:a)
		{
			System.out.println();
		}
		

	}

}
