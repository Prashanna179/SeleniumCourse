
public class ChildClass extends Parentclass
{
	public static void main(String[] args) {
		
		
		ChildClass a = new ChildClass();
		a.colour();
		a.audio();
		a.gear();
		a.engine();
	}

	public void audio()
	{
		
	}
	public void engine()
	{
		System.out.println("Engine available");
	}
	
	public void colour()
	{
		System.out.println(color);
	}
}
