
public class ArrayDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a[]= {1,2,3};
		int min =0;
		
		for(int i=0;i<a.length;i++)
		{
			for(int j=0;j<a.length;j++)
			{
				if(a[i]<a[j])
				{
					if(a[i]<min)
					{
					min=a[i];
					}
				}
			}
		}
		
		System.out.println(min);
	}

}
