
public abstract class AbstractDemo {

	public void dispCollege()
	{
		System.out.println("PSG");
	}
	
	public void dispLocation()
	{
		System.out.println("Coimbatore");
	}
	
	public abstract void dispDepartmentLocation();
	
	public static void main(String[] args) {
		AbstractDemo a = new AbstractDemo();
	}


}
