
public class Parentclass {

	public static void main(String[] args) {
		

	}
	
	public Parentclass()
	{
		System.out.println("parent class constructor");
	}
	
	
	String color = "red";
	
	public void gear()
	{
		System.out.println("Gear available");
	}
	
	public void audio()
	{
		System.out.println("child class audio available");
	}	
}
